    "def open_text(file_name):\n",
    "    try:\n",
    "        with open(file_name) as file:\n",
    "            return file.read()\n",
    "    except FileNotFoundError:\n",
    "        return \"El archivo no existe\"\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    print(open_text(\"hola.txt\"))\n",
    "    print(open_text(\"prueba.txt\"))"
   ]
  }
